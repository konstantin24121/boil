{
  "name": "boil",
  "version": "0.0.0",
  "description": "Fucking awesome superb boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "better-npm-run build",
    "dev": "better-npm-run dev"
  },
  "betterScripts": {
    "dev": {
      "command": "node ./implements/dev-server/dev-server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --colors --progress --display-error-details --config implements/webpack/webpack.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konstantin24121/boil.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/konstantin24121/boil/issues"
  },
  "homepage": "https://github.com/konstantin24121/boil#readme",
  "devDependencies": {
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "awesome-typescript-loader": "^5.2.0",
    "better-npm-run": "^0.1.1",
    "express": "^4.16.3",
    "pretty-error": "^2.1.1",
    "typescript": "^3.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.5",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  }
}
