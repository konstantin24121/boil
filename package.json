{
  "name": "boil",
  "version": "0.0.0",
  "description": "Fucking awesome superb boilerplate",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf ./dist && node ./implements/generate-icons-types.js",
    "build": "better-npm-run buildClient && better-npm-run buildServer",
    "predev": "node ./implements/generate-icons-types.js",
    "dev": "better-npm-run dev",
    "dev:webpack-hmr-server": "node ./implements/servers/hmr-server.js",
    "dev:webpack-ssr-server": "better-npm-run start-dev-ssr",
    "dev:server": "nodemon ./implements/servers/ssr-server.js --watch ./dist",
    "lint": "tslint \"src/app/**/*.ts\" \"src/app/**/*.tsx\" --project ./tsconfig.json",
    "start": "nodemon ./implements/servers/ssr-server.js"
  },
  "betterScripts": {
    "dev": {
      "command": "concurrent --kill-others \"npm run dev:webpack-hmr-server\" \"npm run dev:webpack-ssr-server\" \"npm run dev:server\"",
      "env": {
        "IS_SERVER_BUNDLE": false,
        "NODE_ENV": "development"
      }
    },
    "start-dev-ssr": {
      "command": "webpack --colors --progress --display-error-details --display-max-modules --w --config implements/universal-webpack/webpack.config.server.js",
      "env": {
        "IS_SERVER_BUNDLE": true
      }
    },
    "buildClient": {
      "command": "webpack --colors --progress --display-error-details --config implements/universal-webpack/webpack.config.client.js",
      "env": {
        "IS_SERVER_BUNDLE": false
      }
    },
    "buildServer": {
      "command": "webpack --colors --progress --display-error-details --config implements/universal-webpack/webpack.config.server.js",
      "env": {
        "IS_SERVER_BUNDLE": true
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konstantin24121/boil.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/konstantin24121/boil/issues"
  },
  "homepage": "https://github.com/konstantin24121/boil#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@types/express": "^4.16.0",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/react-helmet": "^5.0.7",
    "@types/react-redux": "^6.0.6",
    "@types/react-router": "^4.0.32",
    "@types/react-router-config": "^1.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux": "^3.6.0",
    "@types/serialize-javascript": "^1.3.2",
    "babel-loader": "^8.0.0",
    "babel-runtime": "^6.26.0",
    "better-npm-run": "^0.1.1",
    "concurrently": "^3.6.1",
    "fork-ts-checker-webpack-plugin": "^0.4.9",
    "nodemon": "^1.18.3",
    "react-hot-loader": "^4.3.5",
    "react-svg-loader": "^2.1.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.1",
    "universal-webpack": "^0.6.6",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.5",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@martin_hotell/rex-tils": "^0.8.0",
    "dotenv": "^6.0.0",
    "emotion": "^9.2.6",
    "emotion-server": "^9.2.6",
    "emotion-theming": "^9.2.6",
    "express": "^4.16.3",
    "pretty-error": "^2.1.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-emotion": "^9.2.6",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-config": "^4.4.0-beta.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^1.5.0"
  }
}
