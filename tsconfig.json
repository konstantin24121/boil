{
  "compilerOptions": {
    "module": "commonjs",
    "jsx": "react",
    "target": "es5",
    "lib": ["dom", "es6", "es2017"],
    "outDir": "dist",
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {
      "modules/*": ["./common/reduck/modules/*"],
      "atoms/*": ["./common/components/atoms/*"],
      "common/*": ["./common/*"]
    }
  }
}
